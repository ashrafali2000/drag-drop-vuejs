[
	{
		"id": "NavbarTop",
		"type": "NavBar",
		"_type": "Widget",
		"category": "Advanced",
		"subcategory": "AButtons",
		"name": "Top NavBar",
		"x": 0,
        "y": 0,
        "w": "$100%",
        "h": "@box-height-s",
        "z": 0,
		"props": {
			"type": "Top",
			"navigation": [
				{"id": 1, "label": "Screen 1", "icon": "", "to": "", "selected": true},
				{"id": 2, "label": "Screen 2", "icon": "", "to": ""},
				{"id": 3, "label": "Screen 3", "icon": "", "to": ""},
				{"id": 4, "label": "Screen 4", "icon": "", "to": ""}
			]
		},
		"has": {
			"backgroundColor": true,
            "border": true,
			"onclick": true,
			"label": true,
			"padding": true
		},
		"style": {
			"fontSize": "@font-size-m",
			"fontFamily": "@font-family",
			"textAlign": "left",
			"letterSpacing": "@letterSpacing",
			"lineHeight": "@lineHeight",
		
			"paddingBottom": 4,
			"paddingTop": 4,
			"paddingLeft": 16,
			"paddingRight": 16,

			"borderWidth": "@border-width-none",
			"borderColor": "@form-border-color",
			"borderStyle": "solid",

			"background": "@button-primary-background",
			"color": "@button-primary-color",

			"gap": -1,
			"itemGap": 8
		},
		"hover": {
			"background": "@button-primary-background:hover",
			"color": "@button-primary-color:hover"	
		},
		"active": {
			"background": "@button-primary-background:hover",
			"color": "@button-primary-color:hover"	
		}
	},
	{
		"id": "NavbarMobile",
		"type": "NavBar",
		"_type": "Widget",
		"category": "Advanced",
		"subcategory": "AButtons",
		"name": "NavBar Mobile",
		"_extends":"NavbarTop",
		"x": 0,
        "y": 0,
        "w": "$100%",
        "h": "@box-height-m",
        "z": 0,
		"props": {
			"type": "MobileBottom",
			"navigation": [
				{"id": 1, "label": "Home", "icon": "mdi mdi-home", "to": "", "selected": true},
				{"id": 2, "label": "Search", "icon": "mdi mdi-magnify", "to": ""},
				{"id": 3, "label": "Account", "icon": "mdi mdi-account-outline", "to": ""}
				
			]
		},
		"style": {
			"gap": -1,
			"paddingBottom": 4,
			"paddingTop": 4,
			"paddingLeft": 16,
			"paddingRight": 16,
			"iconSize": 24,
			"itemGap": 4,
			"fontSize": "@font-size-s"
		}
	},
	{
		"id": "NavbarSide",
		"type": "NavBar",
		"_type": "Widget",
		"category": "Advanced",
		"subcategory": "AButtons",
		"name": "Side NavBar",
		"_extends":"NavbarTop",
		"x": 0,
        "y": 0,
        "w": "@box-width-xxl",
        "h": "$100%",
        "z": 0,
		"props": {
			"type": "Side",
			"navigation": [
				{"id": 1, "label": "Screen 1", "icon": "", "to": "", "selected": true},
				{"id": 2, "label": "Screen 2", "icon": "", "to": ""},
				{"id": 3, "label": "Screen 3", "icon": "", "to": ""},
				{"id": 4, "label": "Screen 4", "icon": "", "to": ""},
				{"id": 5, "label": "Screen 5", "icon": "", "to": ""}
			]
		},
		"style": {
			"gap": 0,
			"paddingBottom": 16,
			"paddingTop": 16,
			"paddingLeft": 4,
			"paddingRight": 4
		}
	}

]