		"padding" : true
	},
	"style" : {
		"popupBorderColor" : "@form-popup-border-color",
		"popupBorderWidth" : "@form-popup-border-width",
		"popupColor" : "@form-popup-color",
		"popupBackground" : "@form-popup-background",
		"popupShadow": "@box-shadow-m",
	
		"selectedOptionColor" : "@form-popup-color:hover",
		"selectedOptionBackground": "@form-popup-background:hover"
